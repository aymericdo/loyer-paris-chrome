{
  "name": "Encadrement loyers paris",
  "version": "1.5",
  "description": "Savoir si le prix est l√©gal ou non. #rendslargent",
  "permissions": [
    "storage",
    "tabs"
  ],
  "page_action": {
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "48": "images/favicon-48x48.png",
      "128": "images/favicon-128x128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.seloger.com/annonces/locations/appartement/*",
        "https://www.leboncoin.fr/locations/*",
        "https://www.loueragile.fr/alert_result*"
      ],
      "js": [
        "custom.js"
      ],
      "css": [
        "custom.css"
      ]
    }
  ],
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "48": "images/favicon-48x48.png",
    "128": "images/favicon-128x128.png"
  },
  "manifest_version": 2
}